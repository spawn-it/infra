{
    "so3": {
      "provider": "docker",
      "container_name": "spawn-it-so3",
      "image": "quay.io/minio/minio:latest",
      "root_user": "minioadmin",
      "root_password": "minioadmin",
      "ports": {
        "9000": "9000",
        "9001": "9001"
      },
      "env_vars": {},
      "command": [ "server", "/data", "--console-address", ":9001" ]
    },
  
    "backend": {
      "provider": "docker",
      "container_name": "spawn-it-backend",
      "image": "ghcr.io/spawn-it/backend:latest",
      "ports": {
        "8000": "8080"
      },
      "env_vars": {
        "SO3_URL": "http://localhost:9000",
        "SO3_CONSOLE_URL": "http://localhost:9001",
        "SO3_ACCESS_KEY": "minioadmin",
        "SO3_SECRET_KEY": "minioadmin",
        "SO3_BUCKET": "tfstates"
      },
      "command" : [ "uvicorn", "main:app", "--host", "0.0.0.0", "--port", "8000"
    ]
    }
  }
  